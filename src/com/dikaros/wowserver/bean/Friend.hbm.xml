<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
       "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.dikaros.wowserver.bean.Friend" table="friends">
	    <composite-id>
	        <key-property name="hostId" column="host_id"></key-property>
	        <key-property name="friendId" column="friend_id"></key-property>
	    </composite-id>
		<property name="friendMark" column="friend_mark" />

	</class>

</hibernate-mapping>
       